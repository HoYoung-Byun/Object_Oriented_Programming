import java.util.Scanner;

public class Week_2 {

	public static void main(String[] args) {
		
		// 02_1. Money
		
		Scanner scan = new Scanner(System.in) ;
		
		int money = scan.nextInt();
		
		int _50000, _10000, _5000, _1000, _500, _100, _100, _50, _10 ;
		
		_50000 = money / 50000 ;
		
		_10000 = (money - 50000*_50000)/10000 ;
		
		
		
		
		System.out.println();
		scan.close();
		
	}

}
