// 05_4. Call by Reference

public class Week_5_04 {
	
	
	public static void main(String[] args) {
		int a = 2; 		
		
		cube(a);
		Double a_prime_Double = Week_5_04.a_prime ;
		System.out.println(a_prime_Double.intValue());
	}
	
	static double a_prime;
	
	static void cube(int a) {
	
	   Week_5_04.a_prime = Math.pow(a, 3) ;
	    
	}

}
